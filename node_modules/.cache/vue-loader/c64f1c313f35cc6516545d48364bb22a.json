selectionReact,rect)){\r\n\t\t\t\t\t\ta = a + $(this).text()+\",\"\r\n\t\t\t\t\t\t\r\n\t\t\t\t\t\t$(this).addClass('selected');\r\n\t\t            }else{\r\n\t\t                $(this).removeClass('selected');\r\n\t\t            }\r\n\t\t        });\r\n\t\t\t\tconsole.log(\"选中的方框为：\"+a)\r\n\t\t    }\r\n\t\t\r\n\t\t$(window).on({\r\n\t\t    mousemove:function(e){\r\n\t\t        if(e.which===1){\r\n\t\t            rector.show();\r\n\t\t            resizeToPoint(e.pageX, e.pageY);\r\n\t\t            \r\n\t\t        }\r\n\t\t    },\r\n\t\t    mouseup:function(){\r\n\t\t       handleRectSelection();\r\n\t\t    },\r\n\t\t    mousedown:function(e){\r\n\t\t        startX = e.pageX;\r\n\t\t        startY = e.pageY;\r\n\t\t        rector.css({\r\n\t\t            top: startY+'px',\r\n\t\t            left: startX+'px',\r\n\t\t        });\r\n\t\t    }\r\n\t\t});\r\n\t}\r\n}\r\n    \r\n</script>\r\n\r\n<style scoped>\r\n    #kz{\r\n        display:flex;\r\n        width:250px;\r\n    }\r\n    .t1{\r\n        width:50px;\r\n        height:50px;\r\n        margin:10px;\r\n        border:1px solid black;\r\n    }\r\n    .selected{\r\n\t    background-color: #FF0;\r\n\t}\r\n</style>"]}]}